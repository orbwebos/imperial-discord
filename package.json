{
  "name": "imperial-discord",
  "version": "0.2.0",
  "description": "A small Discord bot framework, mostly for personal use.",
  "exports": {
    "types": "./dist/esm/index.d.ts",
    "import": "./dist/esm/index.js",
    "require": "./dist/commonjs/index.js"
  },
  "main": "dist/commonjs/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Orbwebos/imperial-discord.git"
  },
  "files": [
    "/dist"
  ],
  "keywords": [
    "discord",
    "framework",
    "bot"
  ],
  "author": "Orbwebos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Orbwebos/imperial-discord/issues"
  },
  "homepage": "https://github.com/Orbwebos/imperial-discord#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "discord.js": "^14.3.0",
    "pino": "^8.0.0"
  }
}
